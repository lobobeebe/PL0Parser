import java.util.Set;
import Utility.SetRepUtility;
import AST.*;

//=========================================================================**
// ReadS attribute
//=========================================================================**

aspect ReadS
{
    ReadS implements ElementaryBlock;

    //=========================================================================**
    // inFlows()
    //=========================================================================**

    //=========================================================================**
    // init()
    //=========================================================================**

    eq  ReadS.init() = getLabel();

    //=========================================================================**
    // blocks()
    //=========================================================================**

    eq ReadS.blocks() = SetRepUtility.singleton(this);

    //=========================================================================**
    // finals()
    //=========================================================================**

    eq ReadS.finals() = SetRepUtility.singleton(getLabel());

    //=========================================================================**
    // outFlows()
    //=========================================================================**

    //=========================================================================**
    // unparse()
    //=========================================================================**

    eq ReadS.unparse()
    {
        return "[read " + getVar() + "]^" + getLabel().unparse();
    }
}
