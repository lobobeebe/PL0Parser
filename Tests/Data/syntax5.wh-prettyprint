{
    [i := (0 - 2)]^1;
    if [(i < 0)]^4 then
    {
        [x := 1]^2    
    } else
    {
        [x := ((2 / 7) + 4)]^3    
    };
    while [((i != 0) or (i > 0))]^10 do
    {
        [skip]^5;
        if [(i > 0)]^9 then
        {
            [x := 1]^6;
            [skip]^7        
        } else
        {
            [x := x]^8        
        }    
    }
}